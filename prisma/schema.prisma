generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Project {
  id               String   @id @default(cuid())
  title            String   @unique
  shortDescription String
  technologies     String[]
  folder           String   @unique
  description      Json
  features         Json
}
